{
  "extends": [
    "../../.eslintrc.json"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.ts"
      ],
      "rules": {
        "no-case-declarations": "off",
        "@typescript-eslint/ban-ts-comment": "off",
        "import/default": "off",
        "import/named": "off"
      }
    }
  ]
}
